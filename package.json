{
  "name": "foodly",
  "version": "1.0.0",
  "description": "e-commerce web app to but healthy food",
  "main": "./server/server.js",
  "scripts": {
    "build-client": "cd client && npm run build",
    "server": "nodemon server/server.js --ignore ",
    "client": "npm start --prefix client",
